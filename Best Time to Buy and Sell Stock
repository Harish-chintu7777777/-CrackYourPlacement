class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        n = len(prices)
        profit = 0
        mini = prices[0]
        for i in range(1,n):
            if prices[i] - mini > 0:
                if prices[i] - mini > profit:
                    profit = prices[i] - mini
            else:
                mini = min(mini,prices[i])
        return profit
        
